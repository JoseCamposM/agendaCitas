@page
@model MyApp.Namespace.formularioPruebaModel
@{
}



   
    <div class="containerCita">
       @{ for (int i = 0; i < Model.nameClientList.Count; i++){
        <article class="tarjetaCita">
            
            <time datetime="2023-10-17T15:30s">@Model.timeList[i] </time>  
            
            <div class="detallesCita">
                <h2>Nombre: @Model.nameClientList[i]</h2>
                <p>Asunto: @Model.asuntoList[i]</p> 
                <p>Persona Asignada: @Model.namePerson</p>
                <p>Cita confirmada: @Model.confirmacion</p>
                <p>Num. Tel: @Model.telUser</p>
                <a href=""> Ver mas</a>
            </div>
        </article>
        }}
    </div>



